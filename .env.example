# ============================================
# 🤖 AI模型API配置
# ============================================

# OpenAI API 配置
# 官方API: https://api.openai.com/v1
# 代理服务: 填入你的代理地址
OPENAI_API_BASE="https://api.openai.com/v1"
OPENAI_API_KEY=""

# Google Gemini API 配置
# 获取方式: https://makersuite.google.com/app/apikey
GEMINI_API_KEY=""


# ============================================
# 🔐 TinySoft 数据源配置（可选）
# ============================================

# 如果你使用 TinySoft（同花顺）数据源，请填写以下信息
TSL_USERNAME=""
TSL_PASSWORD=""
TSL_SERVER="tsl.tinysoft.com.cn"
TSL_PORT="443"


# ============================================
# ⚙️ 系统配置
# ============================================

# 运行时环境文件路径（无需配置，代码会自动使用默认值 ./runtime_env.json）
# 如需自定义，可取消注释并设置绝对路径
# RUNTIME_ENV_PATH="./runtime_env.json"

# Agent 最大推理步数
AGENT_MAX_STEP=30


# ============================================
# 🌐 HTTP 服务端口配置（可选）
# ============================================

MATH_HTTP_PORT=8000
SEARCH_HTTP_PORT=8001
TRADE_HTTP_PORT=8002
GETPRICE_HTTP_PORT=8003


# ============================================
# 📅 日期范围配置（可选，通常由配置文件控制）
# ============================================

# INIT_DATE="2025-10-01"
# END_DATE="2025-10-31"
