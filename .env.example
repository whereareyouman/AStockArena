# ============ TinySoft 配置 ============
TSL_USERNAME=your_tsl_username
TSL_PASSWORD=your_tsl_password
TSL_SERVER=tsl.tinysoft.com.cn
TSL_PORT=443

# ============ 日期范围配置 ============
# 如果设置，会覆盖 configs/default_config.json 中的日期
INIT_DATE=your_init_date
END_DATE=your_end_date

# ============ Gemini API Key ============
GEMINI_API_KEY=your_gemini_api_key

# ============ OpenAI 配置 ============
OPENAI_API_KEY=your_openai_api_key
OPENAI_API_BASE=https://api.openai.com/v1

# ============ Qwen ============
QWEN_API_KEY='sk-your-qwen-api-key'

# ============ 前端是否使用Tinysoft ============
USE_LOCAL_DATA_ONLY=false

# ============ 多模型运行模式 ============
PARALLEL_RUN=true

# ============ SharedPrefetchCoordinator（多模型共享预抓取） ============
# 锁等待超时（秒）。默认 15。设为 0 表示“无限等待”，可避免抢锁导致 fallback。
SHARED_PREFETCH_LOCK_TIMEOUT=0

# ============ 实时模式配置 ============
# wait: 提前启动，自动等待到 10:30/11:30/14:00 再分别执行
# stop: 只跑到"当前已到的时点"，遇到未来时点就退出（方便 cron 3 次触发）
REALTIME_MODE=wait